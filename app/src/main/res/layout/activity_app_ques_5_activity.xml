<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/app_ques_5_layout"
    tools:context=".questionare.App_ques_5_activity">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/page_namaha___23_ek1"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ProgressBar
            android:id="@+id/progress"
            style="?android:attr/progressBarStyleHorizontal"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="60dp"
            android:layout_marginLeft="60dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="60dp"
            android:layout_marginRight="60dp"
            android:max="100"
            android:progress="98"
            android:progressBackgroundTint="#E0E0E0"
            android:progressTint="#FF4900"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/do_you_have_medical_condition_"
            android:layout_width="227dp"
            android:layout_height="62dp"
            android:layout_gravity="center_horizontal|center"
            android:layout_marginTop="22dp"
            android:fontFamily="@font/circular_bold"
            android:gravity="center"
            android:text="Do you have medical condition ?"
            android:textColor="@color/don_t_just_work_out__work_in__color"
            android:textSize="24sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/progress" />

        <TextView
            android:id="@+id/this_helps_us_personalize_your_experience"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/circular"
            android:text="This helps us personalize your experience"
            android:textColor="@color/this_helps_us_personalize_your_experience_color"
            android:textSize="16sp"
            app:autoSizeTextType="uniform"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/do_you_have_medical_condition_" />

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:scrollbarThumbVertical="@android:color/transparent"
            app:layout_constraintBottom_toTopOf="@+id/rectangle_132"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/this_helps_us_personalize_your_experience">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="vertical">

                <CheckBox
                    android:id="@+id/ques5_cb1"
                    android:layout_width="302dp"
                    android:layout_height="54dp"
                    android:layout_marginTop="34dp"
                    android:background="@drawable/radio_flat_selector"
                    android:button="@android:color/transparent"
                    android:fontFamily="@font/circular"
                    android:gravity="center"
                    android:text="High Blood Pressure"
                    android:textColor="@drawable/radio_flat_text_selector"
                    android:textSize="18sp"></CheckBox>

                <CheckBox
                    android:id="@+id/ques5_cb2"
                    android:layout_width="302dp"
                    android:layout_height="54dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/radio_flat_selector"
                    android:button="@android:color/transparent"
                    android:fontFamily="@font/circular"
                    android:gravity="center"
                    android:text="Cholestrol"
                    android:textColor="@drawable/radio_flat_text_selector"
                    android:textSize="18sp"></CheckBox>

                <CheckBox
                    android:id="@+id/ques5_cb3"
                    android:layout_width="302dp"
                    android:layout_height="54dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/radio_flat_selector"
                    android:button="@android:color/transparent"
                    android:fontFamily="@font/circular"
                    android:gravity="center"
                    android:text="Diabetes"
                    android:textColor="@drawable/radio_flat_text_selector"
                    android:textSize="18sp"></CheckBox>

                <CheckBox
                    android:id="@+id/ques5_cb4"
                    android:layout_width="302dp"
                    android:layout_height="54dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/radio_flat_selector"
                    android:button="@android:color/transparent"
                    android:fontFamily="@font/circular"
                    android:gravity="center"
                    android:text="Back Pain"
                    android:textColor="@drawable/radio_flat_text_selector"
                    android:textSize="18sp"></CheckBox>

                <CheckBox
                    android:id="@+id/ques5_cb5"
                    android:layout_width="302dp"
                    android:layout_height="54dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/radio_flat_selector"
                    android:button="@android:color/transparent"
                    android:fontFamily="@font/circular"
                    android:gravity="center"
                    android:text="Anxiety"
                    android:textColor="@drawable/radio_flat_text_selector"
                    android:textSize="18sp"></CheckBox>

                <CheckBox
                    android:id="@+id/ques5_cb6"
                    android:layout_width="302dp"
                    android:layout_height="54dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/radio_flat_selector"
                    android:button="@android:color/transparent"
                    android:fontFamily="@font/circular"
                    android:gravity="center"
                    android:text="Obesity"
                    android:textColor="@drawable/radio_flat_text_selector"
                    android:textSize="18sp"></CheckBox>

                <CheckBox
                    android:id="@+id/ques5_cb7"
                    android:layout_width="302dp"
                    android:layout_height="54dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/radio_flat_selector"
                    android:button="@android:color/transparent"
                    android:fontFamily="@font/circular"
                    android:gravity="center"
                    android:text="Allergy"
                    android:textColor="@drawable/radio_flat_text_selector"
                    android:textSize="18sp"></CheckBox>

                <CheckBox
                    android:id="@+id/ques5_cb8"
                    android:layout_width="302dp"
                    android:layout_height="54dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/radio_flat_selector"
                    android:button="@android:color/transparent"
                    android:fontFamily="@font/circular"
                    android:gravity="center"
                    android:text="Respiratory Problems"
                    android:textColor="@drawable/radio_flat_text_selector"
                    android:textSize="18sp"></CheckBox>

                <CheckBox
                    android:id="@+id/ques5_cb9"
                    android:layout_width="302dp"
                    android:layout_height="54dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/radio_flat_selector"
                    android:button="@android:color/transparent"
                    android:fontFamily="@font/circular"
                    android:gravity="center"
                    android:text="Thyroid"
                    android:textColor="@drawable/radio_flat_text_selector"
                    android:textSize="18sp"></CheckBox>
                <CheckBox
                    android:id="@+id/ques5_cb10"
                    android:layout_width="302dp"
                    android:layout_height="54dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/radio_flat_selector"
                    android:button="@android:color/transparent"
                    android:fontFamily="@font/circular"
                    android:gravity="center"
                    android:text="Blurred Vision"
                    android:textColor="@drawable/radio_flat_text_selector"
                    android:textSize="18sp"></CheckBox>
                <CheckBox
                    android:id="@+id/ques5_cb11"
                    android:layout_width="302dp"
                    android:layout_height="54dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/radio_flat_selector"
                    android:button="@android:color/transparent"
                    android:fontFamily="@font/circular"
                    android:gravity="center"
                    android:text="Arthritis"
                    android:textColor="@drawable/radio_flat_text_selector"
                    android:textSize="18sp"></CheckBox>
                <CheckBox
                    android:id="@+id/ques5_cb12"
                    android:layout_width="302dp"
                    android:layout_height="54dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/radio_flat_selector"
                    android:button="@android:color/transparent"
                    android:fontFamily="@font/circular"
                    android:gravity="center"
                    android:text="General Discomfort and Fatigue"
                    android:textColor="@drawable/radio_flat_text_selector"
                    android:textSize="18sp"></CheckBox>
                <CheckBox
                    android:id="@+id/ques5_cb13"
                    android:layout_width="302dp"
                    android:layout_height="54dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/radio_flat_selector"
                    android:button="@android:color/transparent"
                    android:fontFamily="@font/circular"
                    android:gravity="center"
                    android:text="Pain in Joint"
                    android:textColor="@drawable/radio_flat_text_selector"
                    android:textSize="18sp"></CheckBox>
                <CheckBox
                    android:id="@+id/ques5_cb14"
                    android:layout_width="302dp"
                    android:layout_height="54dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/radio_flat_selector"
                    android:button="@android:color/transparent"
                    android:fontFamily="@font/circular"
                    android:gravity="center"
                    android:text="Sinusitis"
                    android:textColor="@drawable/radio_flat_text_selector"
                    android:textSize="18sp"></CheckBox>
                <CheckBox
                    android:id="@+id/ques5_cb15"
                    android:layout_width="302dp"
                    android:layout_height="54dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/radio_flat_selector"
                    android:button="@android:color/transparent"
                    android:fontFamily="@font/circular"
                    android:gravity="center"
                    android:text="Depression"
                    android:textColor="@drawable/radio_flat_text_selector"
                    android:textSize="18sp"></CheckBox>
                <CheckBox
                    android:id="@+id/ques5_cb16"
                    android:layout_width="302dp"
                    android:layout_height="54dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/radio_flat_selector"
                    android:button="@android:color/transparent"
                    android:fontFamily="@font/circular"
                    android:gravity="center"
                    android:text="Asthma"
                    android:textColor="@drawable/radio_flat_text_selector"
                    android:textSize="18sp"></CheckBox>
                <CheckBox
                    android:id="@+id/ques5_cb17"
                    android:layout_width="302dp"
                    android:layout_height="54dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/radio_flat_selector"
                    android:button="@android:color/transparent"
                    android:fontFamily="@font/circular"
                    android:gravity="center"
                    android:text="Heart Problem"
                    android:textColor="@drawable/radio_flat_text_selector"
                    android:textSize="18sp"></CheckBox>
                <CheckBox
                    android:id="@+id/ques5_cb18"
                    android:layout_width="302dp"
                    android:layout_height="54dp"
                    android:layout_marginTop="24dp"
                    android:background="@drawable/radio_flat_selector"
                    android:button="@android:color/transparent"
                    android:fontFamily="@font/circular"
                    android:gravity="center"
                    android:text="Urinary tract infection"
                    android:textColor="@drawable/radio_flat_text_selector"
                    android:textSize="18sp"></CheckBox>
                <CheckBox
                    android:id="@+id/ques5_cb19"
                    android:layout_width="302dp"
                    android:layout_height="54dp"
                    android:layout_marginTop="24dp"
                    android:layout_marginBottom="24dp"
                    android:background="@drawable/radio_flat_selector"
                    android:button="@android:color/transparent"
                    android:fontFamily="@font/circular"
                    android:gravity="center"
                    android:text="Nope"
                    android:textColor="@drawable/radio_flat_text_selector"
                    android:textSize="18sp"></CheckBox>
            </LinearLayout>
        </ScrollView>
        <com.google.android.material.card.MaterialCardView
            android:layout_width="216dp"
            android:layout_height="54dp"
            android:background="@color/black"
            android:id="@+id/button_back"
            android:backgroundTint="#303030"
            app:cardCornerRadius="9dp"
            android:elevation="15dp"
            app:layout_constraintBottom_toTopOf="@+id/rectangle_132"
            app:layout_constraintEnd_toStartOf="@+id/rectangle_132"
            app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintStart_toEndOf="@+id/rectangle_132"
            app:layout_constraintTop_toBottomOf="@+id/rectangle_132">

            <Button
                android:id="@+id/ques5_button"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@android:color/transparent"
                android:elevation="15dp"
                android:fontFamily="@font/circular"
                android:text="Continue"
                android:textAllCaps="false"
                android:textColor="#000000"
                android:textSize="18sp"></Button>
        </com.google.android.material.card.MaterialCardView>

        <View
            android:id="@+id/rectangle_132"
            android:layout_width="0dp"
            android:layout_height="110dp"
            android:background="@drawable/rectangle_132_shape"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>