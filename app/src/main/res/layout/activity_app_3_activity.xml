            <ImageView
                android:id="@+id/path_52"
                android:layout_width="12dp"
                android:layout_height="11.500610351562dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="9dp"
                android:layout_marginRight="9dp"
                android:contentDescription="path_52"
                android:elevation="10dp"
                android:src="@drawable/namaha11_phone"
                app:layout_constraintEnd_toEndOf="@+id/ellipse_41"
                app:layout_constraintTop_toTopOf="@+id/ellipse_41" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp"
                android:elevation="10dp"
                android:src="@drawable/google_logo"
                app:layout_constraintEnd_toEndOf="@+id/ellipse_42"
                app:layout_constraintTop_toTopOf="@+id/ellipse_42" />

