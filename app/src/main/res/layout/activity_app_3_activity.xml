                android:layout_height="28dp"
                android:layout_marginStart="27dp"
                android:layout_marginLeft="27dp"
                android:layout_marginTop="13dp"
                android:elevation="10dp"
                android:fontFamily="@font/circular"
                android:text="Sign up with Facebook"
                android:textColor="@color/don_t_just_work_out__work_in__color"
                android:textSize="18sp"
                app:autoSizeTextType="uniform"
                app:layout_constraintStart_toStartOf="@+id/rectangle_117"
                app:layout_constraintTop_toTopOf="@+id/rectangle_117" />

            <TextView
                android:id="@+id/sign_up_with_google_"
                android:layout_width="172dp"
                android:layout_height="28dp"
                android:layout_marginStart="27dp"
                android:layout_marginLeft="27dp"
                android:layout_marginTop="13dp"
                android:elevation="10dp"
                android:fontFamily="@font/circular"
                android:text="Sign up with Google"
                android:textColor="@color/don_t_just_work_out__work_in__color"
                android:textSize="18sp"
                app:autoSizeTextType="uniform"
                app:layout_constraintStart_toStartOf="@+id/rectangle_118"
                app:layout_constraintTop_toTopOf="@+id/rectangle_118" />

            <View
                android:id="@+id/ellipse_42"
                android:layout_width="31dp"
                android:layout_height="31dp"
                android:layout_marginTop="13dp"
                android:layout_marginEnd="17dp"
                android:layout_marginRight="17dp"
                android:background="@drawable/ellipse_23_shape"
                android:elevation="10dp"
                app:layout_constraintEnd_toEndOf="@+id/rectangle_118"
                app:layout_constraintTop_toTopOf="@+id/rectangle_118" />

            <View
                android:id="@+id/ellipse_43"
                android:layout_width="31dp"
                android:layout_height="31dp"
                android:layout_marginTop="13dp"
                android:layout_marginEnd="17dp"
                android:layout_marginRight="17dp"
                android:background="@drawable/ellipse_23_shape"
                android:elevation="10dp"
                app:layout_constraintEnd_toEndOf="@+id/rectangle_117"
                app:layout_constraintTop_toTopOf="@+id/rectangle_117" />

            <ImageView
                android:id="@+id/logo__1__ek1"
                android:layout_width="8.5006713867188dp"
                android:layout_height="17dp"
                android:layout_marginTop="7dp"
                android:layout_marginEnd="11dp"
                android:layout_marginRight="11dp"
                android:contentDescription="logo__1__ek1"
                android:elevation="10dp"
