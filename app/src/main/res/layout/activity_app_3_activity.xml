                android:textSize="18sp"
                app:autoSizeTextType="uniform"
                app:layout_constraintStart_toStartOf="@+id/sign_up_phone_button"
                app:layout_constraintTop_toTopOf="@+id/sign_up_phone_button" />

            <View
                android:id="@+id/ellipse_41"
                android:layout_width="31dp"
                android:layout_height="31dp"
                android:layout_marginTop="13dp"
                android:layout_marginEnd="17dp"
                android:layout_marginRight="17dp"
                android:background="@drawable/ellipse_23_shape"
                android:elevation="10dp"
                app:layout_constraintEnd_toEndOf="@+id/sign_up_phone_button"
                app:layout_constraintTop_toTopOf="@+id/sign_up_phone_button" />

            <View
                android:id="@+id/rectangle_117"
                android:layout_width="315dp"
                android:layout_height="54dp"
                android:layout_marginTop="120dp"
                android:background="@drawable/rectangle_53_shape"
                android:elevation="10dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/glad_to__meet_you__" />

            <View
                android:id="@+id/rectangle_118"
                android:layout_width="315dp"
                android:layout_height="54dp"
                android:layout_marginTop="16dp"
                android:background="@drawable/rectangle_54_shape"
                android:elevation="10dp"
                app:layout_constraintEnd_toEndOf="@+id/rectangle_114"
                app:layout_constraintStart_toStartOf="@+id/rectangle_114"
                app:layout_constraintTop_toBottomOf="@+id/rectangle_117" />

            <TextView
